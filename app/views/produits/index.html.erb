<h1>Listing produits</h1>

<table>
  <thead>
    <tr>
      <th>Ref</th>
      <th>Nom</th>
      <th>Prix</th>
      <th>Datecreation</th>
      <th>Datemodification</th>
      <th>Datesuppression</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @produits.each do |produit| %>
      <tr>
        <td><%= produit.ref %></td>
        <td><%= produit.nom %></td>
        <td><%= produit.prix %></td>
        <td><%= produit.dateCreation %></td>
        <td><%= produit.dateModification %></td>
        <td><%= produit.dateSuppression %></td>
        <td><%= link_to 'Show', produit %></td>
        <td><%= link_to 'Edit', edit_produit_path(produit) %></td>
        <td><%= link_to 'Delete', produit, method: :delete %></td>
        <td><%= link_to 'Test', > </td>
        <%#*data: { confirm: 'Are you sure?' }%>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Produit', new_produit_path %>
